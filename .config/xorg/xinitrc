clash &
fcitx5 &
keymap &
setleds -D +num &
statusbar &
wallpaper &
udiskie &
xcompmgr &
xrdb -merge ~/.config/xorg/xresources &
xset r rate 300 30 s 900 &
xautolock -time 10 -locker slock -notify 300 -notifier "st -F cmatrix -s" &
session=${1:-dwm}
case $session in
	dwm   ) exec dwm ;;
	gnome ) exec gnome-session ;;
	kde   ) exec startplasma-x11 ;;
	xfce  ) exec startxfce4 ;;
	*     ) exec $1 ;;
esac
