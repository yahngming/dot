clash &
fcitx5 &
keymap &
setleds -D +num &
statusbar &
wallpaper &
udiskie &
xcompmgr &
xrdb -merge ~/.config/xorg/xresources &
xset r rate 500 50 s 200 600 &
xss-lock -n cmatrix -- slock &
session=${1:-dwm}
case $session in
	dwm   ) exec dwm ;;
	gnome ) exec gnome-session ;;
	kde   ) exec startplasma-x11 ;;
	xfce  ) exec startxfce4 ;;
	*     ) exec $1 ;;
esac
