clash &
fcitx5 &
keymap &
setleds -D +num &
statusbar &
wallpaper &
udiskie &
xcompmgr &
xset r rate 300 30 s 900 &
xautolock -time 3 -locker slock &
session=${1:-dwm}
case $session in
	dwm   ) exec dwm ;;
	gnome ) exec gnome-session ;;
	kde   ) exec startplasma-x11 ;;
	xfce  ) exec startxfce4 ;;
	*     ) exec $1 ;;
esac
