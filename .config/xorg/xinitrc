clash &
fcitx5 &
setleds -D +num &
setxkbmap -option ctrl:nocaps,shift:both_capslock &
statusbar &
wallpaper &
killall xcape 2>/dev/null ; xcape &
xcompmgr &
xrdb -merge ~/.config/xorg/xresources &
xset r rate 500 50 s 600 &
xsidle slock &
session=${1:-dwm}
case $session in
	dwm   ) exec dwm ;;
	gnome ) exec gnome-session ;;
	kde   ) exec startplasma-x11 ;;
	xfce  ) exec startxfce4 ;;
	*     ) exec $1 ;;
esac
