xrandr --output Virtual1 --mode 1920x1080
xcompmgr &
xwall &
xkey &
xtray &
xset s 600 &
xidle slock &
fcitx5 &
clash &
session=${1:-dwm}
case $session in
	dwm          ) exec dwm ;;
	gnome        ) exec gnome-session ;;
	kde          ) exec startplasma-x11 ;;
	xfce         ) exec startxfce4 ;;
	*            ) exec $1 ;;
esac
