#!/bin/sh

case "$LAUNCHER" in
	dmenu ) opt="-noi" ;;
esac

menu="\
󰨇 Dashboard\n\
󰌙 Disable proxy\n\
󱊪 Enable or reload proxy\n\
"

case $(printf "$menu" | menu $opt -p "Proxy" "$@") in
	*Dashboard* ) xdg-open http://127.0.0.1:9090/ui/ ;;
	*Disable*   ) tui "proxy off" ;;
	*Enable*    ) tui "proxy on" ;;
esac
