#!/bin/sh

case "$LAUNCHER" in
	dmenu ) opt="-noi" ;;
esac

menu="\
󰨇 Dashboard\n\
󰌙 Proxy OFF\n\
󱊪 Proxy ON\n\
"

case $(printf "$menu" | menu $opt -p "Proxy options:" "$@") in
	*Dashboard* ) browser --new-tab https://clash.razord.top ;;
	*OFF*       ) rm -f ~/.config/proxy ;;
	*ON*        ) touch -f ~/.config/proxy ;;
esac
