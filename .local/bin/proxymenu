#!/bin/sh

case "$LAUNCHER" in
	dmenu ) opt="-noi" ;;
esac

menu="\
󰨇 Dashboard\n\
󱊪 Proxy ON\n\
󰌙 Proxy OFF\n\
"

case $(printf "$menu" | menu $opt -p "Change proxy:" "$@") in
	*Dashboard* ) browser --new-tab https://clash.razord.top ;;
	*ON*        ) touch -p ~/.config/proxy ;;
	*OFF*       ) rm -p ~/.config/proxy ;;
esac
