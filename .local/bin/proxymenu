#!/bin/sh

case "$LAUNCHER" in
	dmenu ) opt="-noi" ;;
esac

menu="\
󰨇 Open dashboard\n\
󰌙 Turn OFF\n\
󱊪 Turn ON\n\
"

case $(printf "$menu" | menu $opt -p "Proxy" "$@") in
	*dashboard* ) browser --new-tab https://clash.razord.top ;;
	*OFF*       ) rm -f ~/.config/proxy ;;
	*ON*        ) touch -f ~/.config/proxy ;;
esac
