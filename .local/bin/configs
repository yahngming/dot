#!/bin/sh

curl https://raw.githubusercontent.com/yahngming/dot/main/.local/bin/configs -o ~/.local/bin/configs
curl https://raw.githubusercontent.com/yahngming/dot/main/.local/bin/wallpaper -o ~/.local/bin/wallpaper

curl https://raw.githubusercontent.com/yahngming/dot/main/.config/bash/bash_profile -o ~/.config/bash/bash_profile
curl https://raw.githubusercontent.com/yahngming/dot/main/.config/vim/vimrc -o ~/.config/vim/vimrc
curl https://raw.githubusercontent.com/yahngming/dot/main/.config/xorg/xinitrc -o ~/.config/xorg/xinitrc
