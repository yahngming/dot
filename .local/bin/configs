#!/bin/sh

curl https://raw.githubusercontent.com/yahngming/linux/main/.local/bin/configs -o ~/.local/bin/configs
curl https://raw.githubusercontent.com/yahngming/linux/main/.local/bin/wallpaper -o ~/.local/bin/wallpaper

curl https://raw.githubusercontent.com/yahngming/linux/main/.bash_profile -o ~/.bash_profile
curl https://raw.githubusercontent.com/yahngming/linux/main/.vimrc -o ~/.vimrc
curl https://raw.githubusercontent.com/yahngming/linux/main/.xinitrc -o ~/.xinitrc

curl https://raw.githubusercontent.com/yahngming/linux/main/.config/bash/bash_profile -o ~/.config/bash/bash_profile
curl https://raw.githubusercontent.com/yahngming/linux/main/.config/newsboat/urls -o ~/.config/newsboat/urls
curl https://raw.githubusercontent.com/yahngming/linux/main/.config/vim/vimrc -o ~/.config/vim/vimrc
curl https://raw.githubusercontent.com/yahngming/linux/main/.config/xorg/xinitrc -o ~/.config/xorg/xinitrc
