#!/bin/sh

case "$TERMINAL" in
	foot                            ) opt="-a" ;;
	*                               ) opt="-c" ;;
esac

case "$@" in
	*about*|dot|*proxy*|tzselect|up ) class="center" ;;
	*telnet*                        ) class="fullscreen" ;;
esac

$TERMINAL $opt $class -e $SHELL -c "cat ~/.cache/wal/sequences; $@"
