#!/bin/sh

case "$@" in
	about|dot|up ) class="center" ;;
	ttesvr       ) class="fullscreen" ;;
esac
if [ ! -z "$class" ]; then
	case "$TERMINAL" in
		*foot*    ) TERMINAL="foot -a $class" ;;
		*st|xfce* ) TERMINAL="st -c $class" ;;
	esac
fi

$TERMINAL -e $SHELL -c "cat ~/.cache/wal/sequences ; $@"
