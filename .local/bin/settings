#!/bin/sh

case "$(printf "WLAN\nBT\nDisplays\nMount Samba\nMount USB\nUnmount USB" | dmenu)" in
	'WLAN'        ) wifimenu ;;
	'BT'          ) btmenu ;;
	'Displays'    ) dispmenu ;;
	"Mount Samba" ) nasmenu ;;
	'Mount USB'   ) mountmenu ;;
	'Unmount USB' ) umountmenu ;;
	*             ) exit 1 ;;
esac
