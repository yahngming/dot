#!/bin/sh

case "$(printf "WLAN\nBluetooth\nDisplays\nMount Samba\nMount USB\nUnmount USB" | dmenu)" in
	'WLAN'        ) wifimenu ;;
	'Bluetooth'   ) btmenu ;;
	'Monitors'    ) dispmenu ;;
	"Mount Samba" ) nasmenu ;;
	'Mount USB'   ) mountmenu ;;
	'Unmount USB' ) umountmenu ;;
	*             ) exit 1 ;;
esac
