#!/bin/sh

cd ~/.local/src/clash && git pull && cp ~/.local/src/clash/clash ~/.config/clash/config.yaml
cd ~/.local/src/dot && git pull && cp -a ~/.local/src/dot/. ~/ && chmod +x ~/.local/bin/* && chmod 600 ~/.config/aerc/accounts.conf && rm -rf ~/.git ~/postinstall.sh ~/README.md
cd - >/dev/null 2>&1
